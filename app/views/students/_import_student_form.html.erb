<div class="box-body">
  <%= form_tag import_student_path, multipart: true do %>
    <div class="form-group">
      <%= label_tag "File" %>
      <%= file_field_tag :file %>
    </div>
    <%= link_to export_template_student_path(format: :xlsx), class: "btn btn-default" do %>
      <i class="fa fa-file-excel-o"></i> Get file template
    <% end %>
    <%= submit_tag "submit", class: "btn btn-success" %>
  <% end %>
</div>
