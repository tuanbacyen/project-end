<section class="content">
  <div class="box">
    <div class="box-header center-attr">
      <h3 class="box-title">List Users: <%= current_user.name_school %></h3>
    </div>
    <div>
      <%= link_to new_admin_user_path, class: "btn btn-lg" do %>
        <i class="fa fa-user-plus"></i> New users
      <% end %>
      <%= link_to import_export_index_path(format: "xlsx"), class: "btn btn-success" do %>
        <i class="fa fa-file-excel-o"></i> Export to excell
      <% end %>
    </div>
    <hr>
    <%= render "grid_users" %>
  </div>
</section>
